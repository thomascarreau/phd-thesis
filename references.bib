@article{Goriely2005,
title = "Further explorations of {Skyrme}–{Hartree}–{Fock}–{Bogoliubov} mass formulas. {IV}: {Neutron}-matter constraint",
journal = "Nuclear Physics A",
volume = "750",
number = "2",
pages = "425 - 443",
